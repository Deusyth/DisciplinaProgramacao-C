#include <stdio.h>

int main() {
    int mat[2][3], soma = 0;

    printf("Digite os elementos da matriz 2x3:\n");
    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 3; j++) {
            scanf("%d", &mat[i][j]);
            soma += mat[i][j];
        }

    printf("Soma de todos os elementos: %d\n", soma);

    return 0;
}
