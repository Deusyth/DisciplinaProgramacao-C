#include <stdio.h>

int main() {
    int A[5], B[5];

    printf("Digite 5 valores para o vetor A:\n");
    for (int i = 0; i < 5; i++) {
        printf("A[%d]: ", i);
        scanf("%d", &A[i]);
        B[i] = A[i]; // Cópia direta
    }

    printf("Vetor B (cópia de A):\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", B[i]);
    }

    return 0;
}
