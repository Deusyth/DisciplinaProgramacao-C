#include <stdio.h>

int main() {
    int a, b, c, maior;

    printf("Digite tres valores: ");
    scanf("%d %d %d", &a, &b, &c);

    printf("Valores lidos: %d, %d, %d\n", a, b, c);

    maior = a;
    if (b > maior) maior = b;
    if (c > maior) maior = c;

    printf("%d Ã© o maior\n", maior);

    return 0;
}
