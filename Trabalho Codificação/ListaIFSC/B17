#include <stdio.h>

int main() {
    // questao 17 da lista UFSC
    
    int A, B, C, D;
    printf("digite um numero para A: ");
    scanf("%d", &A);
    printf("digite um numero para B: ");
    scanf("%d", &B);
    printf("digite um numero para C: ");
    scanf("%d", &C);
    printf("digite um numero para D: ");
    scanf("%d", &D);
    
    printf("A + B = %d\n", A + B);
    printf("A * B = %d\n", A * B);

    printf("A + C = %d\n", A + C);
    printf("A * C = %d\n", A * C);

    printf("A + D = %d\n", A + D);
    printf("A * D = %d\n", A * D);

    printf("B + C = %d\n", B + C);
    printf("B * C = %d\n", B * C);

    printf("B + D = %d\n", B + D);
    printf("B * D = %d\n", B * D);

    printf("C + D = %d\n", C + D);
    printf("C * D = %d\n", C * D);

    return 0;
}
