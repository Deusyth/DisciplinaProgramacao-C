#include <stdio.h>

int main() {
    int A, B, C;

    printf("Digite A, B e C: ");
    scanf("%d %d %d", &A, &B, &C);

    if (A + B > C) {
        printf("A soma de A e B e maior que C.\n");
    } else if (A + B < C) {
        printf("A soma de A e B e menor que C.\n");
    } else {
        printf("A soma de A e B e igual a C.\n");
    }

    return 0;
}
