#include <stdio.h>

int main() {
    int A[5], B[5], C[10];

    printf("Digite 5 valores para o vetor A:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &A[i]);
        C[i] = A[i];
    }

    printf("Digite 5 valores para o vetor B:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &B[i]);
        C[i + 5] = B[i];
    }

    printf("Vetor C (uniÃ£o de A e B):\n");
    for (int i = 0; i < 10; i++) {
        printf("%d ", C[i]);
    }

    return 0;
}
