#include <stdio.h>

int main() {
    // quest√£o 20 da lista IFSC
    
    int A, B, resultado;

    printf("Digite um numero inteiro para A: ");
    scanf("%d", &A);
    printf("Digite um numero inteiro para B: ");
    scanf("%d", &B);
    
    resultado = (A + B) * (A + B);
    
    printf("o valor do quadrado da some e:: %d\n", resultado);
   
    return 0;
}
