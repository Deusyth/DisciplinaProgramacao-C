#include <stdio.h>

int main() {
    int matricula, ano, semestre;

    printf("Digite o código de matrícula (AASDDD): ");
    scanf("%d", &matricula);

    ano = matricula / 10000;
    semestre = (matricula / 1000) % 10;

    printf("Ano da matrícula: 20%02d\n", ano);
    printf("Semestre: %dº\n", semestre);

    return 0;
}
