#include <stdio.h>

int main() {
    int i, valor, negativos = 0;

    for (i = 0; i < 5; i++) {
        printf("Digite o valor %d: ", i+1);
        scanf("%d", &valor);
        if (valor < 0)
            negativos++;
    }

    printf("Quantidade de valores negativos: %d\n", negativos);

    return 0;
}
