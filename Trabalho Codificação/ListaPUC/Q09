#include <stdio.h>

int main() {
    int i, num, maior, menor;

    printf("Digite o valor 1: ");
    scanf("%d", &num);
    maior = menor = num;

    for (i = 2; i <= 50; i++) {
        printf("Digite o valor %d: ", i);
        scanf("%d", &num);

        if (num > maior) maior = num;
        if (num < menor) menor = num;
    }

    printf("Maior valor: %d\n", maior);
    printf("Menor valor: %d\n", menor);

    return 0;
}
